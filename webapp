    <script>
        // Estado de la aplicaci√≥n
        let appState = {
            currentWeek: 1,
            currentScreen: 'welcome-screen',
            user: null,
            progress: {
                completedWeeks: [1],
                completedDays: 8,
                totalDays: 30,
                exercisesCompleted: 12,
                reflections: 5,
                photosUploaded: 3
            },
            userData: {
                mindsetNotes: '',
                nutritionNotes: '',
                exercisesCompleted: []
            }
        };

        // Funciones de navegaci√≥n
        function showScreen(screenId, weekNumber = null) {
            // Ocultar todas las pantallas
            document.querySelectorAll('.screen').forEach(screen => {
                screen.classList.remove('active');
            });
            
            // Mostrar la pantalla seleccionada
            document.getElementById(screenId).classList.add('active');
            appState.currentScreen = screenId;
            
            // Configurar contenido espec√≠fico por semana
            if (weekNumber && screenId === 'week-detail-screen') {
                setupWeekDetail(weekNumber);
                appState.currentWeek = weekNumber;
            }
        }

        function setupWeekDetail(weekNumber) {
            const mottos = {
                1: "ACTIVA TU FUERZA",
                2: "ELEVA TU MENTE", 
                3: "COME CON CONSCIENCIA",
                4: "INTEGRA Y CONSOLIDA TU PODER"
            };
            
            document.getElementById('week-motto').textContent = mottos[weekNumber];
        }

        function goToWeekDetail() {
            showScreen('week-detail-screen');
        }

        function showWeekIfUnlocked(weekNumber) {
            if (appState.progress.completedWeeks.includes(weekNumber - 1) || weekNumber === 1) {
                showScreen('week-detail-screen', weekNumber);
            } else {
                showNotification('Completa la semana anterior para desbloquear esta semana');
            }
        }

        // Funciones de modal
        function showModal(modalId) {
            document.getElementById(modalId).classList.add('show');
        }

        function closeModal(modalId) {
            document.getElementById(modalId).classList.remove('show');
        }

        // Funciones de autenticaci√≥n
        function login() {
            const email = document.getElementById('email').value;
            const password = document.getElementById('password').value;
            
            if (email && password) {
                appState.user = { email: email, name: 'Queen' };
                closeModal('login-modal');
                showNotification('¬°Bienvenida de nuevo!');
                showScreen('objectives-screen');
            } else {
                showNotification('Por favor completa todos los campos');
            }
        }

        function register() {
            const name = document.getElementById('name').value;
            const email = document.getElementById('reg-email').value;
            const password = document.getElementById('reg-password').value;
            
            if (name && email && password) {
                appState.user = { email: email, name: name };
                closeModal('register-modal');
                showNotification('¬°Cuenta creada exitosamente!');
                showScreen('objectives-screen');
            } else {
                showNotification('Por favor completa todos los campos');
            }
        }

        function googleLogin() {
            appState.user = { email: 'usuario@gmail.com', name: 'Queen' };
            closeModal('google-login-modal');
            showNotification('¬°Conectado con Google!');
            showScreen('objectives-screen');
        }

        function logout() {
            appState.user = null;
            showScreen('welcome-screen');
            showNotification('Sesi√≥n cerrada');
        }

        // Funciones de progreso
        function markDayComplete() {
            // Verificar si todos los ejercicios est√°n marcados
            const checkboxes = document.querySelectorAll('#training-screen input[type="checkbox"]');
            const allChecked = Array.from(checkboxes).every(cb => cb.checked);
            
            if (allChecked) {
                appState.progress.completedDays++;
                appState.progress.exercisesCompleted += checkboxes.length;
                showNotification('¬°D√≠a completado! üéâ');
                updateProgress();
            } else {
                showNotification('Completa todos los ejercicios para marcar el d√≠a como completado');
            }
        }

        function saveMindsetProgress() {
            const notes = document.getElementById('mindset-notes').value;
            appState.userData.mindsetNotes = notes;
            appState.progress.reflections++;
            showNotification('Reflexi√≥n guardada üí≠');
        }

        function saveNutritionProgress() {
            const notes = document.getElementById('nutrition-notes').value;
            appState.userData.nutritionNotes = notes;
            showNotification('Progreso nutricional guardado üçÉ');
        }

        function updateProgress() {
            const percentage = Math.round((appState.progress.completedDays / appState.progress.totalDays) * 100);
            document.querySelectorAll('.progress-fill').forEach(fill => {
                fill.style.width = percentage + '%';
            });
        }

        // Funciones de media
        function playVideo(videoId) {
            const videoContainer = document.getElementById('video-container');
            videoContainer.innerHTML = `
                <iframe width="100%" height="315" 
                    src="https://www.youtube.com/embed/${videoId}" 
                    frameborder="0" 
                    allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" 
                    allowfullscreen
                    style="border-radius: 12px;">
                </iframe>
            `;
            showModal('video-modal');
        }

        function selectPhoto() {
            document.getElementById('photo-input').click();
        }

        // Event listener para el selector de fotos
        document.getElementById('photo-input').addEventListener('change', function(e) {
            if (e.target.files.length > 0) {
                appState.progress.photosUploaded++;
                showNotification('¬°Foto subida correctamente! üì∏');
            }
        });

        // Funciones de navegaci√≥n
        function setActiveNav(element) {
            document.querySelectorAll('.nav-item').forEach(item => {
                item.classList.remove('active');
            });
            element.classList.add('active');
        }

        // Funciones de comunidad
        function joinWhatsApp() {
            // En una implementaci√≥n real, esto abrir√≠a el enlace de WhatsApp
            showNotification('Redirigiendo al grupo de WhatsApp...');
            // window.open('https://chat.whatsapp.com/tu-enlace-grupo', '_blank');
        }

        // Funciones de informaci√≥n
        function getZoomLink() {
            if (appState.progress.completedDays >= 21) {
                showNotification('Enlace de Zoom copiado al portapapeles');
                // navigator.clipboard.writeText('https://zoom.us/j/tu-enlace-reunion');
            }
        }

        function learnMoreOrigen() {
            showNotification('Redirigiendo a la p√°gina de ORIGEN...');
            // En una implementaci√≥n real, esto redirigir√≠a a la p√°gina del programa
        }

        // Sistema de notificaciones
        function showNotification(message) {
            // Crear elemento de notificaci√≥n
            const notification = document.createElement('div');
            notification.style.cssText = `
                position: fixed;
                top: 20px;
                left: 50%;
                transform: translateX(-50%);
                background: #A77D4F;
                color: white;
                padding: 15px 25px;
                border-radius: 25px;
                z-index: 3000;
                font-family: 'Poppins', sans-serif;
                font-weight: 500;
                box-shadow: 0 8px 25px rgba(167, 125, 79, 0.3);
                animation: slideDown 0.3s ease-out;
            `;
            
            // Agregar animaci√≥n CSS
            const style = document.createElement('style');
            style.textContent = `
                @keyframes slideDown {
                    from {
                        opacity: 0;
                        transform: translateX(-50%) translateY(-20px);
                    }
                    to {
                        opacity: 1;
                        transform: translateX(-50%) translateY(0);
                    }
                }
            `;
            document.head.appendChild(style);
            
            notification.textContent = message;
            document.body.appendChild(notification);
            
            // Remover despu√©s de 3 segundos
            setTimeout(() => {
                notification.style.animation = 'slideDown 0.3s ease-out reverse';
                setTimeout(() => {
                    document.body.removeChild(notification);
                    document.head.removeChild(style);
                }, 300);
            }, 3000);
        }

        // Cerrar modales al hacer clic fuera
        document.querySelectorAll('.modal').forEach(modal => {
            modal.addEventListener('click', function(e) {
                if (e.target === modal) {
                    modal.classList.remove('show');
                }
            });
        });

        // Inicializar la aplicaci√≥n
        document.addEventListener('DOMContentLoaded', function() {
            updateProgress();
            
            // Simulaci√≥n de datos guardados
            const savedNotes = 'Reflexi√≥n inicial: Me siento emocionada por comenzar este viaje...';
            document.getElementById('mindset-notes').value = savedNotes;
            
            // Mostrar notificaci√≥n de bienvenida
            setTimeout(() => {
                showNotification('¬°Bienvenida a Real Camifit! üëë');
            }, 1000);
        });

        // Manejar el bot√≥n atr√°s del navegador
        window.addEventListener('popstate', function(e) {
            if (appState.currentScreen !== 'welcome-screen') {
                showScreen('welcome-screen');
            }
        });

        // Prevenir zoom en dispositivos m√≥viles
        document.addEventListener('gesturestart', function(e) {
            e.preventDefault();
        });

        // Optimizaci√≥n para PWA
        if ('serviceWorker' in navigator) {
            window.addEventListener('load', function() {
                navigator.serviceWorker.register('/sw.js')
                    .then(function(registration) {
                        console.log('ServiceWorker registration successful');
                    })
                    .catch(function(err) {
                        console.log('ServiceWorker registration failed');
                    });
            });
        }
    </script>
</body>
</html>

